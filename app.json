{
  "expo": {
    "name": "shift-app",
    "slug": "reality-shift",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icons/appstore.png",
    "userInterfaceStyle": "light",
    "scheme": "realityshift",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/shift-icon-transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kwong916999.realityshift",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": "./assets/app-icons/appstore.png"
    },
    "android": {
      "package": "com.kwong916999.realityshift",
      "icon": "./assets/app-icons/playstore.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icons/playstore.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "eed38d7c-d1d6-4964-9348-4cdac15e928a"
      }
    },
    "owner": "kwong916999",
    "plugins": [
      "expo-av"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/eed38d7c-d1d6-4964-9348-4cdac15e928a",
      "fallbackToCacheTimeout": 0
    }
  }
}
